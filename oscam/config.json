{
  "name": "OSCAM",
  "version": "1.0",
  "slug": "oscam",
  "description": "This add-on allows you to have a OSCAM-Server.",
  "arch": ["armv7", "aarch64", "amd64"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "uart": true,
  "ports": {
    "16002/tcp": 16002,
    "18000/tcp": 18000,
    "27789/tcp": 27789
  },
  "ports_description": {
    "16002/tcp": "Web Interface",
    "18000/tcp": "#1 Server",
    "27789/tcp": "#2 Server"
  },
  "map": [
    "config:rw"
  ],
  "hassio_role": "default"
}
